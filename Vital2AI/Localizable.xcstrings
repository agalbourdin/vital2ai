    "privacy_description" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Local processing, your data never leaves your device. Ephemeral files, biometric security. You choose where to share exported files."
