    "medical_disclaimer_description" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Vital2AI provides health data export for informational purposes only. It is not a substitute for professional medical advice, diagnosis, or treatment. Always seek the counsel of a physician or qualified health provider regarding any medical condition. This app strictly formats raw data from HealthKit and does not interpret, analyze, or provide medical insights.\n\n**Experimental Metric**: Heart Rate Deceleration Index is an experimental feature calculated by this app. It is not guaranteed to be correct and has not been clinically validated."
          }
        }
